{
    "name": "wanikani-client",
    "version": "1.0.6",
    "description": "Fully typed client for the WaniKani API",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/broertjeNL/WaniKani/issues"
    },
    "homepage": "https://github.com/broertjeNL/WaniKani",
    "repository": {
        "type": "github",
        "url": "https://github.com/broertjeNL/WaniKani.git"
    },
    "dependencies": {
        "axios": "^1.13.5",
        "qs": "^6.14.1"
    },
    "devDependencies": {
        "@babel/core": "^7.22.5",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@jest/globals": "^29.6.1",
        "@types/jest": "^29.5.3",
        "@types/mock-fs": "^4.13.1",
        "@types/node": "^20.3.1",
        "@types/qs": "^6.14.0",
        "@typescript-eslint/eslint-plugin": "^8.55.0",
        "@typescript-eslint/parser": "^8.55.0",
        "eslint": "^9.0.0",
        "jest": "^29.6.1",
        "mock-fs": "^5.2.0",
        "prettier": "^3.0.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "main": "src/index.ts",
    "scripts": {
        "clean": "rm -rf ./lib",
        "build": "npm run clean && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc -p tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "prepack": "npm run build",
        "test": "jest"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./lib/esm/types/index.d.ts",
                "default": "./lib/esm/index.mjs"
            },
            "require": {
                "types": "./lib/cjs/types/index.d.ts",
                "default": "./lib/cjs/index.js"
            }
        }
    },
    "files": [
        "lib/**/*"
    ]
}
